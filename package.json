{
  "name": "markdown-in-sider",
  "displayName": "markdown in sider",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/index.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "markdown-in-sider",
          "title": "侧边栏文档",
          "icon": "icon.svg"
        }
      ]
    },
    "views": {
      "markdown-in-sider": [
        {
          "id": "markdown-in-sider:sider",
          "type": "webview",
          "name": "侧边栏文档",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "eslint": "^9.11.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
